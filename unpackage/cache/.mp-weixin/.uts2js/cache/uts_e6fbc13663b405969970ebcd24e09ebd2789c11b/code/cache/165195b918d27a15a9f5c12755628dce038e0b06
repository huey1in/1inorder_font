{"code":"import { defineComponent } from \"vue\";\nimport { BASE_URL } from \"@/common/config\";\nexport default defineComponent({\n    data() {\n        return {\n            addresses: [],\n            isSelectMode: false\n        };\n    },\n    onLoad(options = null) {\n        if (options.select === 'true') {\n            this.isSelectMode = true;\n        }\n    },\n    onShow() {\n        this.fetchAddresses();\n    },\n    methods: {\n        fetchAddresses() {\n            const token = uni.getStorageSync('token');\n            if (!token)\n                return null;\n            uni.request({\n                url: `${BASE_URL}/addresses`,\n                method: 'GET',\n                header: new UTSJSONObject({\n                    'Authorization': `Bearer ${token}`\n                }),\n                success: (res = null) => {\n                    if (res.data.success) {\n                        const list = res.data.data.addresses || [];\n                        // 适配后端字段\n                        this.addresses = list.map((item = null) => { return (new UTSJSONObject(Object.assign(Object.assign({}, item), { contact_name: item.name || item.contact_name, contact_phone: item.phone || item.contact_phone, address: item.address || '', detail: item.detail || '' }))); });\n                    }\n                }\n            });\n        },\n        addAddress() {\n            uni.navigateTo({ url: '/pages/address/edit' });\n        },\n        editAddress(item = null) {\n            uni.navigateTo({ url: `/pages/address/edit?id=${item.id}` });\n        },\n        selectAddress(item = null) {\n            if (this.isSelectMode) {\n                // 如果是选择模式，返回上一页并传递选中的地址\n                const pages = getCurrentPages();\n                const prevPage = pages[pages.length - 2];\n                // @ts-ignore\n                if (prevPage.$vm.setAddress) {\n                    // @ts-ignore\n                    prevPage.$vm.setAddress(item);\n                }\n                uni.navigateBack();\n            }\n        }\n    }\n});\n//# sourceMappingURL=D:/order/fontend/pages/address/list.uvue?vue&type=script&lang.uts.js.map","references":[],"uniExtApis":["uni.getStorageSync","uni.request","uni.navigateTo","uni.navigateBack"],"map":"{\"version\":3,\"file\":\"list.uvue?vue&type=script&lang.uts.js\",\"sourceRoot\":\"\",\"sources\":[\"list.uvue?vue&type=script&lang.uts\"],\"names\":[],\"mappings\":\";AACC,OAAO,EAAE,QAAQ,EAAE,wBAA2B;AAE9C,+BAAe;IACd,IAAI;QACH,OAAO;YACN,SAAS,EAAE,EAAgB;YAC3B,YAAY,EAAE,KAAK;SACnB,CAAA;IACF,CAAC;IACD,MAAM,CAAC,cAAY;QAClB,IAAI,OAAO,CAAC,MAAM,KAAK,MAAM,EAAE;YAC9B,IAAI,CAAC,YAAY,GAAG,IAAI,CAAA;SACxB;IACF,CAAC;IACD,MAAM;QACL,IAAI,CAAC,cAAc,EAAE,CAAA;IACtB,CAAC;IACD,OAAO,EAAE;QACR,cAAc;YACb,MAAM,KAAK,GAAG,GAAG,CAAC,cAAc,CAAC,OAAO,CAAC,CAAA;YACzC,IAAI,CAAC,KAAK;gBAAE,YAAM;YAElB,GAAG,CAAC,OAAO,CAAC;gBACX,GAAG,EAAE,GAAG,QAAQ,YAAY;gBAC5B,MAAM,EAAE,KAAK;gBACb,MAAM,oBAAE;oBACP,eAAe,EAAE,UAAU,KAAK,EAAE;iBAClC,CAAA;gBACD,OAAO,EAAE,CAAC,UAAQ;oBACjB,IAAI,GAAG,CAAC,IAAI,CAAC,OAAO,EAAE;wBACrB,MAAM,IAAI,GAAG,GAAG,CAAC,IAAI,CAAC,IAAI,CAAC,SAAS,IAAI,EAAE,CAAA;wBAC1C,SAAS;wBACT,IAAI,CAAC,SAAS,GAAG,IAAI,CAAC,GAAG,CAAC,CAAC,WAAS,OAAK,OAAA,mDACrC,IAAI,KACP,YAAY,EAAE,IAAI,CAAC,IAAI,IAAI,IAAI,CAAC,YAAY,EAC5C,aAAa,EAAE,IAAI,CAAC,KAAK,IAAI,IAAI,CAAC,aAAa,EAC/C,OAAO,EAAE,IAAI,CAAC,OAAO,IAAI,EAAE,EAC3B,MAAM,EAAE,IAAI,CAAC,MAAM,IAAI,EAAE,KACxB,EANuC,CAMvC,CAAC,CAAA;qBACH;gBACF,CAAC;aACD,CAAC,CAAA;QACH,CAAC;QAED,UAAU;YACT,GAAG,CAAC,UAAU,CAAC,EAAE,GAAG,EAAE,qBAAqB,EAAE,CAAC,CAAA;QAC/C,CAAC;QAED,WAAW,CAAC,WAAS;YACpB,GAAG,CAAC,UAAU,CAAC,EAAE,GAAG,EAAE,0BAA0B,IAAI,CAAC,EAAE,EAAE,EAAE,CAAC,CAAA;QAC7D,CAAC;QAED,aAAa,CAAC,WAAS;YACtB,IAAI,IAAI,CAAC,YAAY,EAAE;gBACtB,wBAAwB;gBACxB,MAAM,KAAK,GAAG,eAAe,EAAE,CAAA;gBAC/B,MAAM,QAAQ,GAAG,KAAK,CAAC,KAAK,CAAC,MAAM,GAAG,CAAC,CAAC,CAAA;gBACxC,aAAa;gBACb,IAAI,QAAQ,CAAC,GAAG,CAAC,UAAU,EAAE;oBAC5B,aAAa;oBACb,QAAQ,CAAC,GAAG,CAAC,UAAU,CAAC,IAAI,CAAC,CAAA;iBAC7B;gBACD,GAAG,CAAC,YAAY,EAAE,CAAA;a