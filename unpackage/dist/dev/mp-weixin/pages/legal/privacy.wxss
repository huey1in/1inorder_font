@import "../../uvue.wxss";
:host{display:flex;flex-direction:column}

.container {
		min-height: 100vh;
		background: #FFF5F8;
		padding: 32rpx;
		box-sizing: border-box;
		color: #5D4E6D;
}
.title {
		font-size: 36rpx;
		font-weight: 800;
		color: #FF69B4;
		margin-bottom: 24rpx;
}
.section {
		margin-bottom: 28rpx;
}
.sub-title {
		font-size: 30rpx;
		font-weight: 700;
		margin-bottom: 10rpx;
		display: block;
}
.paragraph {
		font-size: 28rpx;
		line-height: 1.6;
		color: #6b6575;
}
